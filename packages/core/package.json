{
  "name": "kmenu",
  "version": "2.0.3",
  "description": "The perfect âŒ˜K menu",
  "author": "Harsh Singh",
  "license": "MIT",
  "homepage": "https://kmenu.dev",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./filters": {
      "types": "./dist/filters.d.ts",
      "import": "./dist/filters.mjs",
      "require": "./dist/filters.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "size": "size-limit",
    "test": "vitest"
  },
  "keywords": [
    "kmenu",
    "kbar",
    "cmdk",
    "react-cmdk",
    "commandbar",
    "command bar",
    "command menu",
    "command palette",
    "command",
    "palette",
    "menu",
    "headless",
    "accessible",
    "a11y",
    "keyboard",
    "navigation",
    "search"
  ],
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "size-limit": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "6 kB"
    }
  ]
}
