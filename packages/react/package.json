{
  "name": "@kmenu/react",
  "version": "2.0.2",
  "description": "React adapter for kmenu",
  "author": "Harsh Singh",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "homepage": "https://kmenu.dev",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "size": "size-limit"
  },
  "peerDependencies": {
    "kmenu": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "kmenu": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.0.0",
    "size-limit": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "kmenu",
    "kbar",
    "cmdk",
    "react-cmdk",
    "commandbar",
    "command bar",
    "command menu",
    "command palette",
    "command",
    "palette",
    "menu",
    "headless",
    "accessible",
    "a11y",
    "keyboard",
    "navigation",
    "search"
  ],
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "3 kB"
    }
  ]
}
